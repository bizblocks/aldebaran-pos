#############################################################################
# Makefile for building: src
# Generated by qmake (1.07a) (Qt 3.3.8b) on: Fri Oct 23 13:22:57 2009
# Project:  src.pro
# Template: subdirs
# Command: $(QMAKE) -o Makefile src.pro
#############################################################################

MAKEFILE =	Makefile
QMAKE    =	qmake
DEL_FILE =    rm -f
CHK_DIR_EXISTS= test -d
MKDIR    = mkdir -p
INSTALL_FILE= 
INSTALL_DIR = 
SUBTARGETS =	 \
		sub-lib \
		sub-libshell \
		sub-shell

first: all

all: Makefile $(SUBTARGETS)

lib/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "lib" || $(MKDIR) "lib"
	cd lib && $(QMAKE) lib.pro -o $(MAKEFILE)
sub-lib: lib/$(MAKEFILE) FORCE
	cd lib && $(MAKE) -f $(MAKEFILE)

libshell/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "libshell" || $(MKDIR) "libshell"
	cd libshell && $(QMAKE) libshell.pro -o $(MAKEFILE)
sub-libshell: libshell/$(MAKEFILE) FORCE
	cd libshell && $(MAKE) -f $(MAKEFILE)

shell/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "shell" || $(MKDIR) "shell"
	cd shell && $(QMAKE) shell.pro -o $(MAKEFILE)
sub-shell: shell/$(MAKEFILE) FORCE
	cd shell && $(MAKE) -f $(MAKEFILE)

Makefile: src.pro  /usr/share/qt3/mkspecs/default/qmake.conf 
	$(QMAKE) -o Makefile src.pro
qmake: qmake_all
	@$(QMAKE) -o Makefile src.pro

all: $(SUBTARGETS)
qmake_all: lib/$(MAKEFILE) libshell/$(MAKEFILE) shell/$(MAKEFILE)
	 ( [ -d lib ] && cd lib ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
	 ( [ -d libshell ] && cd libshell ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
	 ( [ -d shell ] && cd shell ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
clean uicables mocables uiclean mocclean lexclean yaccclean : qmake_all FORCE
	 ( [ -d lib ] && cd lib ; $(MAKE) -f $(MAKEFILE) $@; ) || true
	 ( [ -d libshell ] && cd libshell ; $(MAKE) -f $(MAKEFILE) $@; ) || true
	 ( [ -d shell ] && cd shell ; $(MAKE) -f $(MAKEFILE) $@; ) || true
uninstall_subdirs: qmake_all FORCE
	 ( [ -d lib ] && cd lib ; $(MAKE) -f $(MAKEFILE) uninstall; ) || true
	 ( [ -d libshell ] && cd libshell ; $(MAKE) -f $(MAKEFILE) uninstall; ) || true
	 ( [ -d shell ] && cd shell ; $(MAKE) -f $(MAKEFILE) uninstall; ) || true
install_subdirs: qmake_all FORCE
	 ( [ -d lib ] && cd lib ; $(MAKE) -f $(MAKEFILE) install; ) || true
	 ( [ -d libshell ] && cd libshell ; $(MAKE) -f $(MAKEFILE) install; ) || true
	 ( [ -d shell ] && cd shell ; $(MAKE) -f $(MAKEFILE) install; ) || true
distclean: qmake_all FORCE
	 ( [ -d lib ] && cd lib ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true
	 ( [ -d libshell ] && cd libshell ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true
	 ( [ -d shell ] && cd shell ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true

install:  install_subdirs

uninstall:  uninstall_subdirs

FORCE:

