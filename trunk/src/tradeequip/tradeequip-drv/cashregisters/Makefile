MAKEFILE =	Makefile
QMAKE    =	qmake
DEL_FILE =    rm -f
CHK_DIR_EXISTS= test -d
MKDIR    = mkdir -p
INSTALL_FILE= 
INSTALL_DIR = 
SUBTARGETS =	 \
		sub-shtrih-fr \
		sub-felix-rk

first: all

all: Makefile $(SUBTARGETS)

shtrih-fr/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "shtrih-fr" || $(MKDIR) "shtrih-fr"
	cd shtrih-fr && $(QMAKE) shtrih-fr.pro -o $(MAKEFILE)
sub-shtrih-fr: shtrih-fr/$(MAKEFILE) FORCE
	cd shtrih-fr && $(MAKE) -f $(MAKEFILE)

felix-rk/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "felix-rk" || $(MKDIR) "felix-rk"
	cd felix-rk && $(QMAKE) felix-rk.pro -o $(MAKEFILE)
sub-felix-rk: felix-rk/$(MAKEFILE) FORCE
	cd felix-rk && $(MAKE) -f $(MAKEFILE)

Makefile: cashregisters.pro  /usr/share/qt3/mkspecs/default/qmake.conf 
	$(QMAKE) -o Makefile cashregisters.pro
qmake: qmake_all
	@$(QMAKE) -o Makefile cashregisters.pro

all: $(SUBTARGETS)
qmake_all: shtrih-fr/$(MAKEFILE) felix-rk/$(MAKEFILE)
	 ( [ -d shtrih-fr ] && cd shtrih-fr ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
	 ( [ -d felix-rk ] && cd felix-rk ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
clean uicables mocables uiclean mocclean lexclean yaccclean : qmake_all FORCE
	 ( [ -d shtrih-fr ] && cd shtrih-fr ; $(MAKE) -f $(MAKEFILE) $@; ) || true
	 ( [ -d felix-rk ] && cd felix-rk ; $(MAKE) -f $(MAKEFILE) $@; ) || true
uninstall_subdirs: qmake_all FORCE
	 ( [ -d shtrih-fr ] && cd shtrih-fr ; $(MAKE) -f $(MAKEFILE) uninstall; ) || true
	 ( [ -d felix-rk ] && cd felix-rk ; $(MAKE) -f $(MAKEFILE) uninstall; ) || true
install_subdirs: qmake_all FORCE
	 ( [ -d shtrih-fr ] && cd shtrih-fr ; $(MAKE) -f $(MAKEFILE) install; ) || true
	 ( [ -d felix-rk ] && cd felix-rk ; $(MAKE) -f $(MAKEFILE) install; ) || true
distclean: qmake_all FORCE
	 ( [ -d shtrih-fr ] && cd shtrih-fr ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true
	 ( [ -d felix-rk ] && cd felix-rk ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true

install:  install_subdirs

uninstall:  uninstall_subdirs

FORCE:

