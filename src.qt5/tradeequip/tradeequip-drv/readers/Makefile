#############################################################################
# Makefile for building: readers
# Generated by qmake (1.07a) (Qt 3.3.8b) on: Fri Oct 23 13:23:50 2009
# Project:  readers.pro
# Template: subdirs
# Command: $(QMAKE) -o Makefile readers.pro
#############################################################################

MAKEFILE =	Makefile
QMAKE    =	qmake
DEL_FILE =    rm -f
CHK_DIR_EXISTS= test -d
MKDIR    = mkdir -p
INSTALL_FILE= 
INSTALL_DIR = 
SUBTARGETS =	 \
		sub-barcode \
		sub-test

first: all

all: Makefile $(SUBTARGETS)

barcode/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "barcode" || $(MKDIR) "barcode"
	cd barcode && $(QMAKE) barcode.pro -o $(MAKEFILE)
sub-barcode: barcode/$(MAKEFILE) FORCE
	cd barcode && $(MAKE) -f $(MAKEFILE)

test/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "test" || $(MKDIR) "test"
	cd test && $(QMAKE) test.pro -o $(MAKEFILE)
sub-test: test/$(MAKEFILE) FORCE
	cd test && $(MAKE) -f $(MAKEFILE)

Makefile: readers.pro  /usr/share/qt3/mkspecs/default/qmake.conf 
	$(QMAKE) -o Makefile readers.pro
qmake: qmake_all
	@$(QMAKE) -o Makefile readers.pro

all: $(SUBTARGETS)
qmake_all: barcode/$(MAKEFILE) test/$(MAKEFILE)
	 ( [ -d barcode ] && cd barcode ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
	 ( [ -d test ] && cd test ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
clean uicables mocables uiclean mocclean lexclean yaccclean : qmake_all FORCE
	 ( [ -d barcode ] && cd barcode ; $(MAKE) -f $(MAKEFILE) $@; ) || true
	 ( [ -d test ] && cd test ; $(MAKE) -f $(MAKEFILE) $@; ) || true
uninstall_subdirs: qmake_all FORCE
	 ( [ -d barcode ] && cd barcode ; $(MAKE) -f $(MAKEFILE) uninstall; ) || true
	 ( [ -d test ] && cd test ; $(MAKE) -f $(MAKEFILE) uninstall; ) || true
install_subdirs: qmake_all FORCE
	 ( [ -d barcode ] && cd barcode ; $(MAKE) -f $(MAKEFILE) install; ) || true
	 ( [ -d test ] && cd test ; $(MAKE) -f $(MAKEFILE) install; ) || true
distclean: qmake_all FORCE
	 ( [ -d barcode ] && cd barcode ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true
	 ( [ -d test ] && cd test ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true

install:  install_subdirs

uninstall:  uninstall_subdirs

FORCE:

