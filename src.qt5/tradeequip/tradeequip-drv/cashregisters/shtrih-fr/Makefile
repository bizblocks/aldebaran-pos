#############################################################################
# Makefile for building: shtrih-fr
# Generated by qmake (1.07a) (Qt 3.3.8b) on: Fri Oct 23 13:23:31 2009
# Project:  shtrih-fr.pro
# Template: subdirs
# Command: $(QMAKE) -o Makefile shtrih-fr.pro
#############################################################################

MAKEFILE =	Makefile
QMAKE    =	qmake
DEL_FILE =    rm -f
CHK_DIR_EXISTS= test -d
MKDIR    = mkdir -p
INSTALL_FILE= 
INSTALL_DIR = 
SUBTARGETS =	 \
		sub-lib \
		sub-test

first: all

all: Makefile $(SUBTARGETS)

lib/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "lib" || $(MKDIR) "lib"
	cd lib && $(QMAKE) lib.pro -o $(MAKEFILE)
sub-lib: lib/$(MAKEFILE) FORCE
	cd lib && $(MAKE) -f $(MAKEFILE)

test/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "test" || $(MKDIR) "test"
	cd test && $(QMAKE) test.pro -o $(MAKEFILE)
sub-test: test/$(MAKEFILE) FORCE
	cd test && $(MAKE) -f $(MAKEFILE)

Makefile: shtrih-fr.pro  /usr/share/qt3/mkspecs/default/qmake.conf 
	$(QMAKE) -o Makefile shtrih-fr.pro
qmake: qmake_all
	@$(QMAKE) -o Makefile shtrih-fr.pro

all: $(SUBTARGETS)
qmake_all: lib/$(MAKEFILE) test/$(MAKEFILE)
	 ( [ -d lib ] && cd lib ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
	 ( [ -d test ] && cd test ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
clean uicables mocables uiclean mocclean lexclean yaccclean : qmake_all FORCE
	 ( [ -d lib ] && cd lib ; $(MAKE) -f $(MAKEFILE) $@; ) || true
	 ( [ -d test ] && cd test ; $(MAKE) -f $(MAKEFILE) $@; ) || true
uninstall_subdirs: qmake_all FORCE
	 ( [ -d lib ] && cd lib ; $(MAKE) -f $(MAKEFILE) uninstall; ) || true
	 ( [ -d test ] && cd test ; $(MAKE) -f $(MAKEFILE) uninstall; ) || true
install_subdirs: qmake_all FORCE
	 ( [ -d lib ] && cd lib ; $(MAKE) -f $(MAKEFILE) install; ) || true
	 ( [ -d test ] && cd test ; $(MAKE) -f $(MAKEFILE) install; ) || true
distclean: qmake_all FORCE
	 ( [ -d lib ] && cd lib ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true
	 ( [ -d test ] && cd test ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true

install:  install_subdirs

uninstall:  uninstall_subdirs

FORCE:

